{
  "font": {
    "name": "Lato",
    "light": "300",
    "regular": "400",
    "bold": "900"
  },
  "colors": {
    "primary": "#e4833e",
    "white": "#ffffff",
    "light": "#ebedff",
    "dark": "#1d3252",
    "black": "#062035",
    "success": "#6aa84f",
    "notice": "#3e97e4",
    "warning": "#e0cd66",
    "error": "#e06666",
    "whiteFix": "#ffffff",
    "pageBgColor": "var(--color-black)",
    "layoutBgColor": "var(--color-black-darker)",
    "boxBgColor": "var(--color-black-darker)",
    "elementBgColor": "var(--color-black-darker)",
    "textColor": "var(--color-white)"
  },
  "radius": "10px",
  "border": "1px solid var(--color-white-10)",
  "contentMaxWidth": "75rem",
  "shadow": "0 0 6px var(--color-black-50)",
  "transition": "all 0.3s ease-in-out",
  "logos": {
    "vertical": {
      "dark": "https://raw.githubusercontent.com/anynines-johannchopin/happy-static/master/a9s/assets/logos/dark_vertical.svg?sanitize=true",
      "light": "https://raw.githubusercontent.com/anynines-johannchopin/happy-static/master/a9s/assets/logos/light_vertical.svg?sanitize=true"
    },
    "horizontal": {
      "dark": "https://raw.githubusercontent.com/anynines-johannchopin/happy-static/master/a9s/assets/logos/dark_horizontal.svg?sanitize=true",
      "light": "https://raw.githubusercontent.com/anynines-johannchopin/happy-static/master/a9s/assets/logos/light_horizontal.svg?sanitize=true"
    }
  },
  "spacing": {
    "baseUnit": "1em",
    "fixedUnit": "1rem"
  },
  "typography": {
    "baseUnit": "1rem",
    "headingLineHeight": "1.8rem",
    "bodyLineHeight": "1.2rem"
  }
}